<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>fornax</ToolCommandName>
    <RollForward>LatestMajor</RollForward>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\Fornax.Core\Fornax.Core.fsproj" />

    <Compile Include="AssemblyInfo.fs" />
    <Compile Include="FSIRefs.fs" />
    <Compile Include="Generator.fs" />
    <Compile Include="Fornax.fs" />
    <None Include="../Fornax.Template/**" Pack="true" Exclude="../Fornax.Template/_public/**" PackagePath="blogTemplate" CopyToOutputDirectory="PreserveNewest" />
    <None Include="paket.references" />
  </ItemGroup>
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>
